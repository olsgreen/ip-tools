{
	"name": "detain/ip-tools",
	"type": "library",
	"description": "PHP IP Tools for manipulation with IPv4 and IPv6",
	"keywords": ["ip", "ipv4", "ipv6", "tools", "utilities", "compare", "match", "subnet", "mask"],
	"license": "MIT",
	"homepage": "https://github.com/detain/php-ip-tools",
	"support": {
		"issues": "https://github.com/detain/php-ip-tools/issues",
		"source": "https://github.com/detain/php-ip-tools"
	},
	"authors": [
		{
			"name": "Joe Huss",
			"email": "detain@interserver.net",
			"homepage": "https://www.interserver.net",
			"role": "Developer"
		},
		{
			"name": "Avtandil Kikabidze aka LONGMAN",
			"email": "akalongman@gmail.com",
			"homepage": "https://longman.me",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.5",
		"ext-bcmath": "*"
	},
	"autoload": {
		"psr-4": {
			"Detain\\IPTools\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"require-dev": {
		"phpunit/phpunit": "~4.1",
		"phpspec/phpspec": "~2.1",
		"squizlabs/php_codesniffer": "~2.3"
	}
